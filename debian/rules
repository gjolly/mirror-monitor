#!/usr/bin/make -f

export DESTDIR := debian/mirror-monitor

%:
	dh $@

override_dh_clean:
	rm -f debian/mirror-monitor.service
	rm -f debian/mirror-monitor.timer
	dh_clean

override_dh_installsystemd:
	install --mode=644 mirror-monitor.service debian/
	install --mode=644 mirror-monitor.timer debian/
